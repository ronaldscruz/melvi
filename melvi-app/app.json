{
  "expo": {
    "name": "Melvi",
    "slug": "melvi-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#2c3e50"
    },
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./src/assets/logo_medium.png",
      "resizeMode": "contain",
      "backgroundColor": "#34495E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
